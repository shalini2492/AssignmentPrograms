 	   for(k=0; k<=6; k++)
 	   {
 		   if(k==count)
 		   {
 			   System.out.print("***");
 		   }
 		   else
 		   {
 			   System.out.print(" ");
 		   }
 	   }
 	   if(i<=5)
 	   {
 		   count--;
 	   }
 	   else if(i<=11)
 	   {
 		   count++;
 	   }
 	   System.out.print("\n");
    }

case 'W':
	for(i=0; i<11; i++)
	{
		for(j=0; j<=1; j++)
		{
			System.out.print("*");
		}
		for(k=8; k<=1; k--)
		{
			System.out.print(" ");
		}
		
		for(l=1; l<=i; l++)
		{
			System.out.print("  ");
		}
		for(j=0; j<=1; j++)
		{
			System.out.print("*");
		}
		for(k=10; k<=1; k--)
		{
			System.out.print(" ");
		}
		for(j=0; j<=1; j++)
		{
			System.out.print("*");
		}
		System.out.print("\n");
	}
  }
}
	
}